<?php $isAdmin = false; ?>
<?php $user =  $this->zfcUserIdentity(); ?>
<?php if(!empty($user)) : ?>
    <?php $roles = $user->getRoles(); ?>
    <?php foreach ($roles as $role) : ?>
      <?php if($role->getRoleId() == "admin") : ?>
        <?php $isAdmin = true; ?>
      <?php endif; ?>  
    <?php endforeach ; ?>
<?php endif; ?>  

<?php if($isAdmin) : ?>
    <div id="debug"><i class="fa fa-bug"></i></div>
<?php endif; ?>